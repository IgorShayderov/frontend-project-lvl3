{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "airbnb-base",
      "plugin:jest/recommended"
    ],

    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": "off",
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "no-param-reassign": "off",
        "no-underscore-dangle": [2, { "allow": ["__filename", "__dirname"] }]
    }
}
